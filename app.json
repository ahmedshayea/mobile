{
  "expo": {
    "name": "medrasaty-mobile",
    "slug": "medrasaty-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_exper.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#191C1D"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true,
        "ExpoLocalization_forcesRTL": true
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#191C1D"
      },
      "package": "com.solomonsh.medrasatymobile"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Cairo-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true,
      "forcesRTL": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8a520cb5-0b91-49ee-a13b-1ae5e3ac194b"
      }
    }
  }
}
